    "class myBinMaxHeap:\n",
    "    def __init__(self, maxsize = 10000):\n",
    "        self.maxsize = maxsize\n",
    "        self.size = 0\n",
    "        self.Heap = [0]*(self.maxsize + 1)\n",
    "        self.Heap[0] = sys.maxsize\n",
    "        self.FRONT = 1\n",
    "    def getLeftChild(self, i):\n",
    "        if i*2 <= self.size:\n",
    "            return i*2\n",
    "        else:\n",
    "            return -1\n",
    "    def getRightChild(self, i):\n",
    "        if i*2+1 <= self.size:\n",
    "            return i*2+1\n",
    "        else:\n",
    "            return -1\n",
    "        \n",
    "    def getParent(self, i):\n",
    "        if i//2 > 0:\n",
    "            return i//2\n",
    "        else:\n",
    "            return -1\n",
    "        \n",
    "    def isLeaf(self, i):\n",
    "        if (i >= self.size//2) and i <= self.size:\n",
